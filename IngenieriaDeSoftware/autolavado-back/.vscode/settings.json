{
    "workbench.colorCustomizations": {
        "titleBar.activeForeground": "#ffffff",
        "titleBar.inactiveForeground": "#ffffff",
        "titleBar.activeBackground": "#34b4eb",
        "titleBar.inactiveBackground": "#34b4eb"
    },
    "editor.fontSize": 14,
    "vetur.validation.template": false,
    "eslint.validate": [
        {
            "language": "vue",
            "autoFix": true
        },
        {
            "language": "html",
            "autoFix": true
        },
        {
            "language": "javascript",
            "autoFix": true
        }
    ],
    "eslint.autoFixOnSave": true,
    "editor.formatOnSave": true
}
